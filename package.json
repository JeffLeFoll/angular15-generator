{
  "name": "angular-generator",
  "version": "1.0.0",
  "description": "CLI tool to generate Angular 1.5 w/ TypeScript Routes and Components like Angular 2 CLI",
  "main": "",
  "bin": {
    "ag": "bin/generator.js"
  },
  "scripts": {
    "test": "mocha ./src/**/*_spec.js --reporter spec --recursive",
    "test:cover": "istanbul cover _mocha -- -u exports -R spec ./src/**/*_spec.js",
    "test:coverAppveyor": "istanbul cover node_modules/mocha/bin/_mocha -- -u exports -R mocha-appveyor-reporter ./src/**/*_spec.js",
    "lint": "eslint .; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JeffLeFoll/angular15-generator.git"
  },
  "bugs": {
    "url": "https://github.com/JeffLeFoll/angular15-generator/issues"
  },
  "homepage": "https://github.com/JeffLeFoll/angular15-generator",
  "keywords": [
    "Angular",
    "1.5",
    "CLI",
    "Typescript"
  ],
  "author": "Jean-Francois Le Foll (@JeffLeFoll)",
  "license": "MIT",
  "engines": {
    "node": ">= 4.1.0"
  },
  "dependencies": {
    "fs-extra": "^0.30.0",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-appveyor-reporter": "^0.2.1"
  }
}
